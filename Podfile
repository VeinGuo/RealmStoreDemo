source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
inhibit_all_warnings!
use_frameworks!

target 'RealmStoreDemo' do
    pod 'CryptoSwift', '~> 0.8.0'	# 加密
    pod 'SwiftyJSON'	# JSON 解析
    pod 'RealmSwift', '~> 3.0.0' # 数据库
    pod 'PKHUD', :git => 'https://github.com/pkluz/PKHUD.git', :branch => 'release/swift4' # HUD 菊花
    pod 'SwifterSwift', '~> 4.0.0' # 一些好用的Extension 方法
end

target 'RealmStoreDemoTests' do
    pod 'CryptoSwift', '~> 0.8.0'	# 加密
    pod 'SwiftyJSON'	# JSON 解析
    pod 'RealmSwift', '~> 3.0.0' # 数据库
    pod 'PKHUD', :git => 'https://github.com/pkluz/PKHUD.git', :branch => 'release/swift4' # HUD 菊花
    pod 'SwifterSwift', '~> 4.0.0' # 一些好用的Extension 方法
end

target 'RealmStoreDemoUITests' do
    pod 'CryptoSwift', '~> 0.8.0'	# 加密
    pod 'SwiftyJSON'	# JSON 解析
    pod 'RealmSwift', '~> 3.0.0' # 数据库
    pod 'PKHUD', :git => 'https://github.com/pkluz/PKHUD.git', :branch => 'release/swift4' # HUD 菊花
    pod 'SwifterSwift', '~> 4.0.0' # 一些好用的Extension 方法
end



